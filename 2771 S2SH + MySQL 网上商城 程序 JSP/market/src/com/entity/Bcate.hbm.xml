<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.entity.Bcate" table="bcate">
		<id name="bcateid" type="java.lang.String">
			<column name="bcateid" length="255" />
			<generator class="uuid.hex"></generator>
		</id>
		<property name="bcatename" type="java.lang.String">
			<column name="bcatename" />
		</property>
	</class>
</hibernate-mapping>